<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mpcronschedule>
            <general>
                <clear_schedule>0 0 * * *</clear_schedule>
                <backend_notification>1</backend_notification>
                <email_notification>1</email_notification>
                <send_from>general</send_from>
                <email_template>mpcronschedule_general_email_template</email_template>
                <schedule>0 * * * *</schedule>
            </general>
        </mpcronschedule>
    </default>
</config>
